{
  "name": "infections",
  "version": "0.0.0",
  "description": "Solves the Infections problem",
  "main": "index.js",
  "scripts": {
    "test": "testem",
    "build": "mkdir -p dist && npm run copyfiles && npm run buildjs",
    "buildjs": "browserify app/index.js -o dist/app.js -t [ babelify --presets [ es2015 ] ]",
    "copyfiles": "cp public/* dist",
    "serve": "watchify  app/index.js -o dist/app.js -t [ babelify --presets [ es2015 ] ] -dv & livereloadify dist & http-server ./dist",
    "livereload": "livereloadify dist"
  },
  "author": "Brian Genisio",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "http-server": "^0.9.0",
    "livereloadify": "^2.0.0",
    "testem": "^1.6.0",
    "watchify": "^3.7.0"
  }
}
